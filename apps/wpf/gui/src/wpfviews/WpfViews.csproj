    <UseWPF>true</UseWPF>
    <AssemblyName>Mov.WpfViews</AssemblyName>
    <RootNamespace>Mov.WpfViews</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Prism.Wpf" Version="8.1.97" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\commons\wpfcontrols\WpfControls.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Components\Tabs\MenuTab.xaml.cs">
