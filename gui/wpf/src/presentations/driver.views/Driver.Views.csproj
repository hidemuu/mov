    <UseWPF>true</UseWPF>
    <AssemblyName>Mov.Driver.Views</AssemblyName>
    <RootNamespace>Mov.Driver.Views</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\commons\wpfviews\WpfViews.csproj" />
    <ProjectReference Include="..\driver.viewmodels\Driver.ViewModels.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DriverView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="DriverView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
